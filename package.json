{
  "name": "went-app",
  "version": "1.0.0",
  "description": "Chattez, commandez, d√©couvrez !",
  "main": "main.js",
  "author": "Went Lab Corporation",
  "license": "MIT",
  "win": {
    "icon": "./img/android-icon-192x192.png"
  },
  "scripts": {
    "postinstall": "install-app-deps",
    "start": "npm install && electron .",
    "pack": "build --dir",
    "dist": "build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/went-lab/wentcorp.git"
  },
  "keywords": [
    "wentcorp"
  ],
  "bugs": {
    "url": "https://github.com/went-lab/wentcorp/issues"
  },
  "homepage": "https://github.com/went-lab/wentcorp#readme",
  "build": {
    "appId": "went-app",
    "category": "wentlabcorporation.wentapp.access.accessingtoourservices",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "build/icon.ico"
    }
  },
  "dependencies": {
      "chokidar": "^3.5.2",
      "drag-drop": "^7.2.0",
      "fs-extra": "^10.0.0",
      "open": "^8.4.0"
  },
  "devDependencies": {
    "electron": "latest",
    "electron-builder": "latest"
  }
